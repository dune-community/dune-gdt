la_backends = eigen_sparse, istl_sparse | expand backends
EIGEN3_FOUND, dune-istl_FOUND | expand backends | cmake_guard

[__static]
TESTCASETYPE = Dune::GDT::LinearElliptic::{testcases}<{grids}>
SPACE_BACKEND = {space_backends}
